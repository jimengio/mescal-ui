{
  "name": "@jimengio/mescal-ui",
  "version": "0.1.6",
  "description": "",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "dev": "webpack-dev-server --hot --config webpack/dev.js",
    "watch": "webpack-dev-server --hot --config webpack/dev.js",
    "dll": "rm -rfv webpack/dll/* && time webpack --config webpack/dll-dev.js",
    "dll-release": "time webpack --config webpack/dll-release.js",
    "release": "rm -rfv dist/* && yarn dll-release && time webpack --progress --config webpack/release.js",
    "build": "yarn release",
    "serve": "http-server dist/ -s",
    "lang-editor": "cd src/lingual/ && locales-editor",
    "compile": "tsc -d --project tsconfig-compile.json --outDir lib/",
    "prepublish": "yarn compile",
    "upload": "rsync -avr --progress dist/ fe.jimu.io:~/repo/jimengio/mescal-ui",
    "postinstall": "rm -rfv node_modules/@types/*/node_modules/@types/react"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jimengio/rex": "^0.1.1",
    "@types/history": "^4.7.2",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^3.0.1",
    "history": "^4.9.0",
    "html-webpack-include-assets-plugin": "^1.0.7",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.0",
    "lorem-ipsum": "^2.0.1",
    "style-loader": "^0.23.1",
    "typescript": "^3.4.2",
    "typescript-styled-plugin": "^0.14.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.2.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.0",
    "webpack-hud": "^0.1.2"
  },
  "dependencies": {
    "@jimengio/fa-icons": "^0.1.10",
    "@jimengio/jimo-icons": "^0.1.12",
    "@jimengio/ruled-router": "^0.2.7",
    "@jimengio/shared-utils": "^0.1.1",
    "antd-mobile": "^2.2.11",
    "dayjs": "*",
    "emotion": "*",
    "font-awesome": "^4.7.0",
